parameters:
	locales:
		default: cs_CZ
	templates: /design/front/default/

php:
	date.timezone: Europe/Prague


nette:
	application:
		errorPresenter: Error
		mapping:
			*: App\Modules\*\Presenters\*Presenter

	session:
		expiration: 14 days

services:
#	- App\Model\UserManager
	- App\RouterFactory
	router: @App\RouterFactory::createRouter(%routes%)
	database: App\Management\EntityManager(%doctrine.connection%, %doctrine.entityDir%, %doctrine.devmode%)
